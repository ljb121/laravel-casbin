{
    "name": "casbin/laravel-adapter",
    "keywords": ["casbin", "laravel", "casbin-adapter", "database"],
    "description": "Use casbin in laravel. ",
    "authors": [
        {
            "name": "TechLee",
            "email": "techlee@qq.com"
        }
    ],
    "license": "Apache-2.0",
    "require": {
    	"laravel/framework": "~5.1",
